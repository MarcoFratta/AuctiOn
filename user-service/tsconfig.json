{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"sourceMap": false,
		"outDir": "dist",
		"noImplicitAny": false,
		"noUnusedParameters": false,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"rootDir": ".",
		"lib": [
			"es6"
		],
		"strict": true,
		"baseUrl": "./src"
	},
	"exclude": [
		"node_modules",
		"dist"
	],
	"include": ["src/**/*",]
}
